{
  "name": "test-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl true",
    "serve:prod": "ng serve --prod",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "postbuild:prod": "cp -r ./dist/testApp /usr/local/var/www/testApp",
    "nginx": "sudo nginx -s stop && sudo nginx && open https://localhost",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "prettier": "prettier --write '**/*.{ts,js,css,html}'",
    "serve": "node serve-script",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run testApp:server:production"
  },
  "private": true,
